<?xml version="1.0" encoding="UTF-8"?>
<project name="templateprj" default="all">
  
  
  <property file="templateprj.properties"/>
  <!-- Uncomment the following property if no tests compilation is needed -->
  <!-- 
  <property name="skip.tests" value="true"/>
   -->
  <property name="javac2.home" value="${idea.home}/lib"/>
  <path id="javac2.classpath">
    <pathelement location="${javac2.home}/javac2.jar"/>
    <pathelement location="${javac2.home}/jdom.jar"/>
    <pathelement location="${javac2.home}/asm.jar"/>
    <pathelement location="${javac2.home}/asm-commons.jar"/>
  </path>
  <taskdef name="javac2" classname="com.intellij.ant.Javac2" classpathref="javac2.classpath"/>
  <taskdef name="instrumentIdeaExtensions" classname="com.intellij.ant.InstrumentIdeaExtensions" classpathref="javac2.classpath"/>
  
  <!-- Compiler options -->
  
  <property name="compiler.debug" value="on"/>
  <property name="compiler.generate.no.warnings" value="off"/>
  <property name="compiler.args" value=""/>
  <property name="compiler.max.memory" value="128m"/>
  <patternset id="ignored.files">
    <exclude name="**/CVS/**"/>
    <exclude name="**/SCCS/**"/>
    <exclude name="**/RCS/**"/>
    <exclude name="**/rcs/**"/>
    <exclude name="**/.DS_Store/**"/>
    <exclude name="**/.svn/**"/>
    <exclude name="**/.sbas/**"/>
    <exclude name="**/.IJI.*/**"/>
    <exclude name="**/vssver.scc/**"/>
    <exclude name="**/vssver2.scc/**"/>
    <exclude name="**/.pyc/**"/>
    <exclude name="**/.pyo/**"/>
    <exclude name="**/*.pyc/**"/>
    <exclude name="**/*.pyo/**"/>
    <exclude name="**/.git/**"/>
    <exclude name="**/*.hprof/**"/>
    <exclude name="**/_svn/**"/>
    <exclude name="**/.hg/**"/>
  </patternset>
  <patternset id="library.patterns">
    <include name="*.zip"/>
    <include name="*.war"/>
    <include name="*.egg"/>
    <include name="*.ear"/>
    <include name="*.swc"/>
    <include name="*.jar"/>
  </patternset>
  <patternset id="compiler.resources">
    <include name="**/?*.properties"/>
    <include name="**/?*.xml"/>
    <include name="**/?*.gif"/>
    <include name="**/?*.png"/>
    <include name="**/?*.jpeg"/>
    <include name="**/?*.jpg"/>
    <include name="**/?*.html"/>
    <include name="**/?*.dtd"/>
    <include name="**/?*.tld"/>
    <include name="**/?*.ftl"/>
    <include name="**/?*.htm"/>
    <include name="**/?*.jsp"/>
    <include name="**/?*.js"/>
  </patternset>
  
  
  <!-- Project Libraries -->
  
  <path id="library.teamcity_open_api_agent.classpath">
    <pathelement location="${path.variable.teamcitydistribution}/devPackage/agent-api.jar"/>
  </path>
  
  <path id="library.teamcity_open_api_common.classpath">
    <pathelement location="${path.variable.teamcitydistribution}/devPackage/common-api.jar"/>
    <pathelement location="${path.variable.teamcitydistribution}/devPackage/runtime-util.jar"/>
  </path>
  
  <path id="library.teamcity_open_api_server.classpath">
    <pathelement location="${path.variable.teamcitydistribution}/devPackage/server-api.jar"/>
  </path>
  
  <path id="library.teamcity_third-party_common.classpath">
    <pathelement location="${path.variable.teamcitydistribution}/webapps/ROOT/WEB-INF/lib/annotations.jar"/>
    <pathelement location="${path.variable.teamcitydistribution}/webapps/ROOT/WEB-INF/lib/openapi.jar"/>
    <pathelement location="${path.variable.teamcitydistribution}/webapps/ROOT/WEB-INF/lib/spring.jar"/>
    <pathelement location="${path.variable.teamcitydistribution}/webapps/ROOT/WEB-INF/lib/util.jar"/>
  </path>
  
  <path id="library.teamcity_third-party_server.classpath">
    <pathelement location="${path.variable.teamcitydistribution}/webapps/ROOT/WEB-INF/lib/spring-webmvc.jar"/>
  </path>
  
  <path id="library.tomcat.classpath">
    <pathelement location="${path.variable.teamcitydistribution}/lib/servlet-api.jar"/>
  </path>
  
  
  <!-- Application Server Libraries -->
  
  <!-- Modules -->
  
  
  <!-- Module templatePrj-common -->
  
  <dirname property="module.templateprj-common.basedir" file="${ant.file}"/>
  
  
  
  <property name="compiler.args.templateprj-common" value="${compiler.args}"/>
  
  <property name="templateprj-common.output.dir" value="${module.templateprj-common.basedir}/out/production/templatePrj-common"/>
  <property name="templateprj-common.testoutput.dir" value="${module.templateprj-common.basedir}/out/test/templatePrj-common"/>
  
  <path id="templateprj-common.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="templateprj-common.module.classpath">
    <path refid="library.teamcity_open_api_common.classpath"/>
    <path refid="library.teamcity_third-party_common.classpath"/>
  </path>
  
  <path id="templateprj-common.runtime.module.classpath">
    <pathelement location="${templateprj-common.output.dir}"/>
    <pathelement location="${templateprj-common.testoutput.dir}"/>
    <path refid="library.teamcity_open_api_common.classpath"/>
    <path refid="library.teamcity_third-party_common.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.templateprj-common">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.templateprj-common">
    <patternset refid="excluded.from.module.templateprj-common"/>
  </patternset>
  
  <path id="templateprj-common.module.sourcepath">
    <dirset dir="${module.templateprj-common.basedir}/common">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.templateprj-common" depends="compile.module.templateprj-common.production,compile.module.templateprj-common.tests" description="Compile module templatePrj-common"/>
  
  <target name="compile.module.templateprj-common.production" description="Compile module templatePrj-common; production classes">
    <mkdir dir="${templateprj-common.output.dir}"/>
    <javac2 destdir="${templateprj-common.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true">
      <compilerarg line="${compiler.args.templateprj-common}"/>
      <bootclasspath refid="templateprj-common.module.bootclasspath"/>
      <classpath refid="templateprj-common.module.classpath"/>
      <src refid="templateprj-common.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.templateprj-common"/>
    </javac2>
    
    <copy todir="${templateprj-common.output.dir}">
      <fileset dir="${module.templateprj-common.basedir}/common/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.templateprj-common.tests" depends="compile.module.templateprj-common.production" description="compile module templatePrj-common; test classes" unless="skip.tests"/>
  
  <target name="clean.module.templateprj-common" description="cleanup module">
    <delete dir="${templateprj-common.output.dir}"/>
    <delete dir="${templateprj-common.testoutput.dir}"/>
  </target>
  
  
  <!-- Module templatePrj-agent -->
  
  <dirname property="module.templateprj-agent.basedir" file="${ant.file}"/>
  
  
  
  <property name="compiler.args.templateprj-agent" value="${compiler.args}"/>
  
  <property name="templateprj-agent.output.dir" value="${module.templateprj-agent.basedir}/out/production/templatePrj-agent"/>
  <property name="templateprj-agent.testoutput.dir" value="${module.templateprj-agent.basedir}/out/test/templatePrj-agent"/>
  
  <path id="templateprj-agent.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="templateprj-agent.module.classpath">
    <path refid="library.teamcity_open_api_agent.classpath"/>
    <path refid="library.teamcity_open_api_common.classpath"/>
    <path refid="library.teamcity_third-party_common.classpath"/>
    <pathelement location="${templateprj-common.output.dir}"/>
    <pathelement location="${templateprj-common.testoutput.dir}"/>
  </path>
  
  <path id="templateprj-agent.runtime.module.classpath">
    <pathelement location="${templateprj-agent.output.dir}"/>
    <pathelement location="${templateprj-agent.testoutput.dir}"/>
    <path refid="library.teamcity_open_api_agent.classpath"/>
    <path refid="library.teamcity_open_api_common.classpath"/>
    <path refid="library.teamcity_third-party_common.classpath"/>
    <path refid="templateprj-common.runtime.module.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.templateprj-agent">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.templateprj-agent">
    <patternset refid="excluded.from.module.templateprj-agent"/>
  </patternset>
  
  <path id="templateprj-agent.module.sourcepath">
    <dirset dir="${module.templateprj-agent.basedir}/agent">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.templateprj-agent" depends="compile.module.templateprj-agent.production,compile.module.templateprj-agent.tests" description="Compile module templatePrj-agent"/>
  
  <target name="compile.module.templateprj-agent.production" depends="compile.module.templateprj-common" description="Compile module templatePrj-agent; production classes">
    <mkdir dir="${templateprj-agent.output.dir}"/>
    <javac2 destdir="${templateprj-agent.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true">
      <compilerarg line="${compiler.args.templateprj-agent}"/>
      <bootclasspath refid="templateprj-agent.module.bootclasspath"/>
      <classpath refid="templateprj-agent.module.classpath"/>
      <src refid="templateprj-agent.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.templateprj-agent"/>
    </javac2>
    
    <copy todir="${templateprj-agent.output.dir}">
      <fileset dir="${module.templateprj-agent.basedir}/agent/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.templateprj-agent.tests" depends="compile.module.templateprj-agent.production" description="compile module templatePrj-agent; test classes" unless="skip.tests"/>
  
  <target name="clean.module.templateprj-agent" description="cleanup module">
    <delete dir="${templateprj-agent.output.dir}"/>
    <delete dir="${templateprj-agent.testoutput.dir}"/>
  </target>
  
  
  <!-- Module templatePrj-server -->
  
  <dirname property="module.templateprj-server.basedir" file="${ant.file}"/>
  
  
  
  <property name="compiler.args.templateprj-server" value="${compiler.args}"/>
  
  <property name="templateprj-server.output.dir" value="${module.templateprj-server.basedir}/out/production/templatePrj-server"/>
  <property name="templateprj-server.testoutput.dir" value="${module.templateprj-server.basedir}/out/test/templatePrj-server"/>
  
  <path id="templateprj-server.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="templateprj-server.module.classpath">
    <path refid="library.tomcat.classpath"/>
    <path refid="library.teamcity_open_api_server.classpath"/>
    <path refid="library.teamcity_open_api_common.classpath"/>
    <path refid="library.teamcity_third-party_common.classpath"/>
    <path refid="library.teamcity_third-party_server.classpath"/>
    <pathelement location="${templateprj-common.output.dir}"/>
    <pathelement location="${templateprj-common.testoutput.dir}"/>
  </path>
  
  <path id="templateprj-server.runtime.module.classpath">
    <pathelement location="${templateprj-server.output.dir}"/>
    <pathelement location="${templateprj-server.testoutput.dir}"/>
    <path refid="library.tomcat.classpath"/>
    <path refid="library.teamcity_open_api_server.classpath"/>
    <path refid="library.teamcity_open_api_common.classpath"/>
    <path refid="library.teamcity_third-party_common.classpath"/>
    <path refid="library.teamcity_third-party_server.classpath"/>
    <path refid="templateprj-common.runtime.module.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.templateprj-server">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.templateprj-server">
    <patternset refid="excluded.from.module.templateprj-server"/>
  </patternset>
  
  <path id="templateprj-server.module.sourcepath">
    <dirset dir="${module.templateprj-server.basedir}/server">
      <include name="src"/>
      <include name="resources"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.templateprj-server" depends="compile.module.templateprj-server.production,compile.module.templateprj-server.tests" description="Compile module templatePrj-server"/>
  
  <target name="compile.module.templateprj-server.production" depends="compile.module.templateprj-common" description="Compile module templatePrj-server; production classes">
    <mkdir dir="${templateprj-server.output.dir}"/>
    <javac2 destdir="${templateprj-server.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true">
      <compilerarg line="${compiler.args.templateprj-server}"/>
      <bootclasspath refid="templateprj-server.module.bootclasspath"/>
      <classpath refid="templateprj-server.module.classpath"/>
      <src refid="templateprj-server.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.templateprj-server"/>
    </javac2>
    
    <copy todir="${templateprj-server.output.dir}">
      <fileset dir="${module.templateprj-server.basedir}/server/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.templateprj-server.basedir}/server/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.templateprj-server.tests" depends="compile.module.templateprj-server.production" description="compile module templatePrj-server; test classes" unless="skip.tests"/>
  
  <target name="clean.module.templateprj-server" description="cleanup module">
    <delete dir="${templateprj-server.output.dir}"/>
    <delete dir="${templateprj-server.testoutput.dir}"/>
  </target>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.templateprj-common, clean.module.templateprj-agent, clean.module.templateprj-server" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.templateprj-common, compile.module.templateprj-agent, compile.module.templateprj-server" description="build all modules"/>
  
  <target name="all" depends="build.modules" description="build all"/>
</project>